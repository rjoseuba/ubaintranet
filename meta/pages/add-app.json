{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Name",
                              "component": "Name",
                              "name": "NameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 200,
                              "type": "textbox",
                              "name": "Name",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${app.Name}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Description",
                              "component": "Description",
                              "name": "DescriptionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 200,
                              "type": "textbox",
                              "name": "Description",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${app.Description}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Ulr",
                              "component": "Ulr",
                              "name": "UlrLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 200,
                              "type": "textbox",
                              "name": "Ulr",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${app.Ulr}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Class Logo",
                              "component": "ClassLogo",
                              "name": "ClassLogoLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "ClassLogo",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${app.ClassLogo}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Category",
                              "component": "Category",
                              "name": "CategoryLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 50,
                              "type": "textbox",
                              "name": "Category",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${app.Category}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Last Update",
                              "component": "LastUpdate",
                              "name": "LastUpdateLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "DateTime?",
                              "dateFormat": "MM/dd/yyyy",
                              "type": "datepicker",
                              "name": "LastUpdate",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${app.LastUpdate}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new App!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "KKK.createApp",
                        "parameters": [
                          {
                            "name": "App",
                            "value": "${app}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${app}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${app}",
                  "visible": "${app != null}",
                  "itemType": "Intranet.Models.Kkk.App"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "app",
        "type": "set",
        "value": "new Intranet.Models.Kkk.App(){}",
        "valueType": "Intranet.Models.Kkk.App"
      }
    ]
  },
  "name": "Add App",
  "layout": "Main"
}