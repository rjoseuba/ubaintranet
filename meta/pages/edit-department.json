                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update Department",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          },
                          {
                            "name": "hasChanges",
                            "type": "set",
                            "value": "result is DbUpdateConcurrencyException",
                            "valueType": "bool"
                          },
                          {
                            "name": "canEdit",
                            "type": "set",
                            "condition": "!(result is DbUpdateConcurrencyException)",
                            "value": "false",
                            "valueType": "bool"
                          }
                        ],
                        "name": "KKK.updateDepartment",
                        "parameters": [
                          {
                            "name": "DeptoId",
                            "value": "${parameters.DeptoId}"
                          },
                          {
                            "name": "Department",
                            "value": "${department}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${department}",
                            "condition": ""
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "itemType": "Intranet.Models.Kkk.Department",
                  "data": "${department}",
                  "visible": "${department != null && canEdit}"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "hasChanges",
        "type": "set",
        "value": "false",
        "valueType": "bool"
      },
      {
        "name": "canEdit",
        "type": "set",
        "value": "true",
        "valueType": "bool"
      },
      {
        "name": "KKK.getDepartmentByDeptoId",
        "parameters": [
          {
            "name": "DeptoId",
            "value": "${parameters.DeptoId}"
          }
        ],
        "then": [
          {
            "name": "department",
            "type": "set",
            "value": "${result}",
            "valueType": "Intranet.Models.Kkk.Department"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "KKK.getDepartments",
        "parameters": [],
        "then": [
          {
            "name": "getDepartmentsForDeptoIdResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<Intranet.Models.Kkk.Department>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Department",
  "layout": "Main"
}